			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "math.Random(5, 7)"
			},
			"minecraft:particle_initial_spin": {
				"rotation_rate": "math.Random(-1.5, 1.5)"
			},
			"minecraft:particle_initial_speed": "math.Random(1, 1.5)",
